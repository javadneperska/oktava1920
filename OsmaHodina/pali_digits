 private static void fhodina8_4_Cifry()
    {
        int n = sc.nextInt();
        int largest = 0;
        int smallest = 9;

        while(n != 0)
        {
            int r = n % 10;            
            largest = Math.max(r, largest);            
            n = n / 10;
        }
        System.out.println(largest + " " + smallest);

    }
