package com.company;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);


        int [] poleA = {1,3,5,6,4,9};
        System.out.println(Arrays.toString(poleA));
        System.out.println("Zadajte index: ");
        int index = sc.nextInt();
        poleA[index] = 0;
        System.out.println(Arrays.toString(poleA));

    }
}
